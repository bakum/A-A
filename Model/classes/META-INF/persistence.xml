<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
  <persistence-unit name="Model">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:/app/jdbc/jdbc/bakumDS</jta-data-source>
    <class>ua.bakum.aa.model.ejb.WlsSettings</class>
    <class>ua.bakum.aa.model.ejb.Groupmembers</class>
    <class>ua.bakum.aa.model.ejb.Groups</class>
    <class>ua.bakum.aa.model.ejb.Users</class>
    <properties>
      <property name="eclipselink.target-server" value="WebLogic_10"/>
      <property name="javax.persistence.jtaDataSource" value="java:/app/jdbc/jdbc/bakumDS"/>
    </properties>
  </persistence-unit>
</persistence>
